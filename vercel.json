{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs22.x"
    },
    "imports/**/*.ts": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/$1"
    },
    {
      "src": "/imports/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/imports/$1"
    }
  ]
}